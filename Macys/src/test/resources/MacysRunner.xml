<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Group2 Selenium Project " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="catalina"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="79.0"/>
    <parameter name="url" value="https://www.macys.com/"/>

    <test name = "Macys Home">
        <classes>
            <class name="home.TestMacysHomePage"/>
            <class name="home.TestMacysLogIn"/>
            <class name="home.search.SearchTest"/>
        </classes>
    </test>
</suite>